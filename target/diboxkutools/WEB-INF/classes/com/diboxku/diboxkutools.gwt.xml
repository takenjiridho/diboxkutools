<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='diboxkutools'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User' />

  <!-- We need the JUnit module in the main module,               -->
  <!-- otherwise eclipse complains (Google plugin bug?)           -->
  <inherits name='com.google.gwt.junit.JUnit' />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard' />
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.diboxku.client.diboxkutools' />
  <inherits name='com.extjs.gxt.ui.GXT' />
<!-- uncomment if production -->
    <stylesheet src="/diboxkutools/css/gxt-all.css" />
 	<stylesheet src="/diboxkutools/resources/css/gxt-all.css" />
	<stylesheet src="/diboxkutools/css/resources.css" />
	<stylesheet src="/diboxkutools/css/icons.css" />
	<stylesheet src="/diboxkutools/js/ext/resources/css/ext-all.css" />
	<script src="/diboxkutools/js/ext/adapter/ext/ext-base.js" />
	<script src="/diboxkutools/js/ext/ext-all.js" /> 
<!-- uncomment if development -->	
	<!-- <stylesheet src="/css/gxt-all.css" />
 	<stylesheet src="/resources/css/gxt-all.css" />
	<stylesheet src="/css/resources.css" />
	<stylesheet src="/css/icons.css" />
	<stylesheet src="/js/ext/resources/css/ext-all.css" />
	<script src="/js/ext/adapter/ext/ext-base.js" />
	 --><script src="/js/ext/ext-all.js" />
	



  <!-- Specify the paths for translatable code                    -->
  <source path='client' />
  <source path='shared' />

</module>
